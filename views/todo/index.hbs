
      {{> header}}


      <div class="row my-4 p-2 ">
        
      {{#each todos}}

        <div class="card col-md-3 m-2" >
          <a href="/todo">
              <div class="card-body">
                <h4 class="card-title">{{this.title}} </h4>
                <p class="card-text">{{this.description}}</p>
              </div>
          </a>
        </div>
        {{/each}}
      </div>


      {{> footer}}